{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" : [
        { 
            "name": "client",
            "image" : "jjfagan19/multi-client",
            "hostname" : "client",
            "essential" : false
        },
        { 
            "name": "api",
            "image" : "jjfagan19/multi-api",
            "hostname" : "api",
            "essential" : false
        },
        { 
            "name": "worker",
            "image" : "jjfagan19/multi-worker",
            "hostname" : "worker",
            "essential" : false
        },
        { 
            "name": "nginx",
            "image" : "jjfagan19/multi-nginx",
            "essential" : true,
            "portMappings" : [
              {
                "hostPort" : 80,
                "containerPort" : 80
              }
            ],
            "links"  : [ "client", "api" ]
       
        }
     ]

}
